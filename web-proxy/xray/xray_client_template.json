            "show": false,
            "serverName": "<a.vps.com>",
            "fingerprint": "chrome",
            "publicKey": "{{REALITY_PBK}}",
            "shortId": "{{REALITY_SID}}",
            "spiderX": "/"
          },
          "sockopt": {
            "tcpFastOpen": true,
            "tcpMptcp": true,
            "tcpNoDelay": true
          }
        },
        "mux": {
          "enabled": true,
          "concurrency": -1,
          "xudpConcurrency": 16,
          "xudpProxyUDP443": "reject"
        }
